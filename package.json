    "react-dom": "^18.2.0",
    "react-modal": "^3.15.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.4.0",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
